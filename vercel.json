{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "ignoreCommand": "git diff --quiet HEAD^ HEAD ./src/assets/"
  }